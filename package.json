    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.0.3",
    "rollup": "^4.3.0",
    "rollup-plugin-copy": "^3.5.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
