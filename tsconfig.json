{
  "compilerOptions": {
    "lib": ["ES2022"],
    "target": "ES2022",
    "declaration": true,
    "declarationDir": "lib",
    "moduleResolution":"nodenext",
    "module": "NodeNext",
    "moduleDetection": "auto",
    "outDir": "lib"
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"],
  "exclude": ["node_modules/**/*"],
}
